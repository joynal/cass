{
  "env": {
    "node": true,
    "browser": true,
    "mocha": true,
    "jest": true
  },
  "globals": {
    "after": true,
    "afterEach": true,
    "before": true,
    "beforeEach": true,
    "describe": true,
    "expect": true,
    "it": true
  },
  "parserOptions": {
    "ecmaVersion": 9
  },
  "extends": ["airbnb-base"],
  "rules": {
    "no-console": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": ["error", { "argsIgnorePattern": "next" }],
    "no-use-before-define": ["error", { "variables": false }],
    "no-multi-str": 0,
    "no-continue": 0
  }
}
